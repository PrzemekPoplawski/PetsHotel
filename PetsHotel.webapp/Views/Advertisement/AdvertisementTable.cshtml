@model PetsHotel.webapp.ViewModels.Advertisement.AdvertisementTemplate

<table>
    <tr>
        <td>
            @Html.LabelFor(x => x.Title)
        </td>
        <td>
            @Html.TextBoxFor(x => x.Title)
        </td>
    </tr>
    <tr>
        <td>
            <br />
        </td>
    </tr>
    <tr>
        <td>
            @Html.LabelFor(x => x.AnimalTypeId)
           
        </td>
        <td>
            @*@Html.EnumDropDownListFor(
        model=>model,
        "Wybierz typ zwierzaka")}*@
            @Html.TextBoxFor(x => x.AnimalTypeId)
        </td>
    </tr>
    <tr>
        <td>
            <br />
        </td>
    </tr>
    <tr>
        <td>
            @Html.LabelFor(x => x.Adress)
           
        </td>
        <td>
            @Html.TextBoxFor(x => x.Adress, new { placeholder = "Ulica i miejscowość" })
        </td>
    </tr>
    <tr>
        <td>
            <br />
        </td>
    </tr>
    <tr>
        <td>
            @Html.LabelFor(x => x.ValidFrom)
            
        </td>
        <td>
            <input type="date" />
        </td>
    </tr>
    <tr>
        <td>
            <br />
        </td>
    </tr>
    <tr>
        <td>
            @Html.LabelFor(x => x.ValidTo)
            
        </td>
        <td>
            <input type="date" />
        </td>
    </tr>
    <tr>
        <td>
            <br />
        </td>
    </tr>
    <tr>
        <td>
            @Html.LabelFor(x => x.Description)
        </td>
        <td>
            @Html.TextAreaFor(x => x.Description)
        </td>
    </tr>
</table>










